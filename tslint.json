{
    "rules":
        {
            "adjacent-overload-signatures":
                [
                    false
                ],
            "member-access":
                [
                    true,
                    "check-accessor",
                    "check-constructor"
                ],
            "member-ordering":
                [
                    true,
                    {
                        "order":
                            [
                                "private-instance-field",
                                "private-static-field",
                                "protected-instance-field",
                                "protected-static-field",
                                "public-instance-field",
                                "public-static-field",
                                "private-instance-method",
                                "private-static-method",
                                "protected-instance-method",
                                "protected-static-method",
                                "constructor",
                                "public-instance-method",
                                "public-static-method"
                            ]
                    }
                ],
            "no-any":
                [
                    false
                ],
            "no-inferrable-types":
                [
                    false
                ],
            "no-internal-module":
                [
                    true
                ],
            "no-namespace":
                [
                    true
                ],
            "no-reference":
                [
                    true
                ],
            "no-var-requires":
                [
                    false
                ],
            "typedef":
                [
                    true,
                    [
                        "arrow-parameter",
                        "call-signature",
                        "member-variable-declaration",
                        "parameter",
                        "property-declaration",
                        "variable-declaration"
                    ]
                ],
            "typedef-whitespace":
                [
                    true
                ],

            "ban":
                [
                    false
                ],
            "curly":
                [
                    true
                ],
            "forin":
                [
                    false
                ],
            "label-position":
                [
                    true
                ],
            "no-arg":
                [
                    true
                ],
            "no-bitwise":
                [
                    false
                ],
            "no-conditional-assignment":
                [
                    true
                ],
            "no-console":
                [
                    true
                ],
            "no-construct":
                [
                    true
                ],
            "no-debugger":
                [
                    true
                ],
            "no-duplicate-variable":
                [
                    true
                ],
            "no-empty":
                [
                    true
                ],
            "no-eval":
                [
                    true
                ],
            "no-for-in-array":
                [
                    true
                ],
            "no-invalid-this":
                [
                    false
                ],
            "no-null-keyword":
                [
                    false
                ],
            "no-shadowed-variable":
                [
                    true
                ],
            "no-string-literal":
                [
                    true
                ],
            "no-switch-case-fall-through":
                [
                    true
                ],
            "no-unreachable":
                [
                    true
                ],
            "no-unsafe-finally":
                [
                    true
                ],
            "no-unused-expression":
                [
                    true
                ],
            "no-unused-new":
                [
                    true
                ],
            "no-unused-variable":
                [
                    true
                ],
            "no-use-before-declare":
                [
                    true
                ],
            "no-var-keyword":
                [
                    true
                ],
            "radix":
                [
                    true
                ],
            "switch-default":
                [
                    false
                ],
            "triple-equals":
                [
                    true,
                    "allow-null-check"
                ],
            "use-isnan":
                [
                    true
                ],
            "use-strict":
                [
                    false
                ],

            "eofline":
                [
                    true
                ],
            "indent":
                [
                    true,
                    "spaces"
                ],
            "linebreak-style":
                [
                    true,
                    "LF"
                ],
            "max-file-line-count":
                [
                    false
                ],
            "max-line-length":
                [
                    true,
                    150
                ],
            "no-default-export":
                [
                    false
                ],
            "no-mergeable-namespace":
                [
                    true
                ],
            "no-require-imports":
                [
                    false
                ],
            "no-trailing-whitespace":
                [
                    true
                ],
            "object-literal-sort-keys":
                [
                    true
                ],
            "ordered-imports":
                [
                    true
                ],
            "trailing-comma":
                [
                    true,
                    {
                        "multiline": "never",
                        "singleline": "never"
                    }
                ],

            "align":
                [
                    false
                ],
            "arrow-parens":
                [
                    true
                ],
            "class-name":
                [
                    true
                ],
            "comment-format":
                [
                    true,
                    "check-space"
                ],
            "file-header":
                [
                    false
                ],
            "interface-name":
                [
                    false
                ],
            "jsdoc-format":
                [
                    true
                ],
            "new-parens":
                [
                    true
                ],
            "no-angle-bracket-type-assertion":
                [
                    true
                ],
            "no-consecutive-blank-lines":
                [
                    true
                ],
            "no-constructor-vars":
                [
                    true
                ],
            "object-literal-key-quotes":
                [
                    true,
                    "as-needed"
                ],
            "object-literal-shorthand":
                [
                    true
                ],
            "one-line":
                [
                    false
                ],
            "one-variable-per-declaration":
                [
                    true,
                    "ignore-for-loop"
                ],
            "only-arrow-functions":
                [
                    false
                ],
            "quotemark":
                [
                    true,
                    "single"
                ],
            "semicolon":
                [
                    true,
                    "always"
                ],
            "variable-name":
                [
                    true,
                    "ban-keywords",
                    "check-format",
                    "allow-pascal-case"
                ],
            "whitespace":
                [
                    true,
                    "check-branch",
                    "check-decl",
                    "check-module",
                    "check-operator",
                    "check-separator",
                    "check-type",
                    "check-typecast"
                ]
        }
}
